{
  "name": "node-api-ecommerce",
  "version": "1.0.0",
  "description": "A ecommerce platform for web APIs using Node",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "11.12.0"
  },
  "scripts": {
    "start": "node cluster.js",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon",
    "test": "npm run test:all",
    "test:all": "npm run test:unit && npm run test:features",
    "test:unit": "cross-env NODE_PATH=. NODE_ENV=test mocha --opts test/mocha.opts.unit",
    "test:features": "cross-env NODE_PATH=. NODE_ENV=test mocha --opts test/mocha.opts.features",
    "test:single": "cross-env NODE_PATH=. NODE_ENV=test mocha",
    "coverage": "cross-env NODE_PATH=. NODE_ENV=test nyc mocha --opts test/mocha.opts.unit",
    "lint": "eslint {src,test,config}/**/*.js",
    "sequelize": "cross-env NODE_PATH=. sequelize",
    "console": "cross-env NODE_PATH=. node src/interfaces/console/index.js",
    "heroku-postbuild": "NODE_ENV=production NODE_PATH=. sequelize db:migrate --url=$DATABASE_URL",
    "pm2": "pm2",
    "cleanup": "node scripts/cleanup.js"
  },
  "repository": {},
  "author": "Kapil Jain <kapil.jain@ipragmatech.com>",
  "license": "MIT",
  "dependencies": {
    "awilix": "^3.0.9",
    "awilix-express": "^0.11.0",
    "bcrypt": "^5.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.1",
    "cross-env": "^3.2.3",
    "del": "^2.2.2",
    "dotenv": "^4.0.0",
    "email-templates": "^5.0.4",
    "email-validator": "^2.0.4",
    "eslint": "^4.7.2",
    "express": "^4.16.4",
    "express-paginate": "^1.0.0",
    "express-session": "^1.15.6",
    "express-status-monitor": "^0.1.9",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.5.0",
    "log4js": "^1.1.1",
    "method-override": "^2.3.7",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^6.0.0",
    "nodemailer-promise": "^2.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^6.1.3",
    "pm2": "^2.4.2",
    "pug": "^2.0.3",
    "sequelize": "^5.8.6",
    "structure": "^1.2.0",
    "swagger-ui-express": "^2.0.14",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-change": "^2.1.2",
    "chance": "^1.0.6",
    "dirty-chai": "^2.0.1",
    "factory-girl": "^4.0.0",
    "listr": "^0.11.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.18.10",
    "nyc": "^11.2.1",
    "replace-in-file": "^2.5.0",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  }
}
